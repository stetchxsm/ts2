{
  "name": "drwhite-group-website",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Dr White Group - Travel, Education & Trade Services",
  "homepage": "https://drwhitegroup.com",
  "scripts": {
    "dev": "vite",
    "build": "vite build --config vite.config.prod.ts",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build --config vite.config.prod.ts && npx vite-bundle-analyzer dist",
    "preview": "vite preview",
    "preview:build": "npm run build && npm run preview",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist node_modules/.vite",
    "start": "npm run dev"
  },
  "dependencies": {
    "@radix-ui/react-dropdown-menu": "^2.1.1",
